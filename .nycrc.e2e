{
  "extends": "@istanbuljs/nyc-config-typescript",
  "exclude": [ "coverage", "scripts/**/*", "e2e/**/*" ],
  "all": true,
  "cache": false,
  "check-coverage": true,
  "per-file": false,
  "lines": 80,
  "statements": 80,
  "functions": 70,
  "branches": 100,
  "reporter": [
    "html",
    "text-summary"
  ]
}
