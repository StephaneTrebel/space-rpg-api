{
  "extends": "@istanbuljs/nyc-config-typescript",
  "exclude": [
    "coverage*",
    "scripts/**/*",
    "src/**/*unit*"
  ],
  "all": false,
  "cache": false,
  "check-coverage": false,
  "per-file": false,
  "lines": 80,
  "statements": 80,
  "functions": 70,
  "branches": 100,
  "report-dir": "./coverage-e2e",
  "reporter": [
    "html",
    "text-summary"
  ]
}
