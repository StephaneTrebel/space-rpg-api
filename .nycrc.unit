{
  "include": [
    "src/**/*",
    "dist/**/*"
  ],
  "exclude": [
    "**/*.d.ts"
  ],
  "extension": [".ts"],
  "cache": false,
  "all": true,
  "sourceMap": true,
  "check-coverage": true,
  "per-file": false,
  "lines": 100,
  "statements": 100,
  "functions": 100,
  "branches": 100,
  "reporter": [
    "html",
    "lcov",
    "text-summary"
  ]
}
